{
  "name": "vue-webble",
  "version": "0.1.0",
  "description": "A Web Bluetooth UI component for Vue.js",
  "main": "dist/vue-webble.umd.js",
  "module": "dist/vue-webble.esm.js",
  "unpkg": "dist/vue-webble.min.js",
  "browser": {
    "./sfc": "src/components/VueWebble.vue"
  },
  "files": [
    "example/*",
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "homepage": "https://github.com/hanhdt/vue-webble",
  "bugs": "https://github.com/hanhdt/vue-webble/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanhdt/vue-webble.git"
  },
  "author": {
    "name": "Hanh D. TRAN",
    "email": "tranduchanh.ms@gmail.com",
    "url": "https://hanhdt.github.io"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "web bluetooth",
    "ble",
    "vue component",
    "js",
    "javascript"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./sample/src/main.js --open",
    "build": "vue-cli-service build --target lib --name vue-webble ./src/index.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-webble.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-webble.min.js",
    "lint": "vue-cli-service lint",
    "test:unit": "jest --clearCache && vue-cli-service test:unit"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-plugin-unit-jest": "^3.3.0",
    "@vue/cli-service": "^3.1.4",
    "babel-core": "~7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.11.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-sass": "^0.9.3",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.0.1",
    "vue": "^2.5.21",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
